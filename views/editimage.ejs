<h3>Edit Image</h3>

<% image %>

<form method="POST" action="/auth/profile/img/<%=image.id%>/edit?_method=PUT">
  Title: <input type="text" name="title" value="<%=image.title%>">
  Description: <input type="text" name="description" value="<%=image.description%>">
  <input type= "submit" value="Submit">
</form>
